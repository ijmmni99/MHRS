<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700" rel="stylesheet">
    <title>@ViewBag.Title</title>
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)


    <!-- Bootstrap core CSS -->
    <link href="~/Scripts/css/bootstrap.min.css" rel="stylesheet">

    <!-- Additional CSS Files -->
    <link rel="stylesheet" href="~/Scripts/css/fontawesome.css">
    <link rel="stylesheet" href="~/Scripts/css/style.css">
    <link rel="stylesheet" href="~/Scripts/css/owl.css">
</head>
<body class="is-preload">
    <!-- Wrapper -->
    <div id="wrapper">

        <!-- Main -->
        <div id="main">
            <div class="inner">

                <!-- Header -->
                <header id="header">
                    <div class="logo">
                        <a href="index.html">Admin Dashboard</a>
                    </div>
                </header>

                <!-- Banner -->
                <section class="main-banner">
                    @RenderBody()
                </section>

                <!-- Services -->
            </div>
        </div>

        <!-- Sidebar -->
        <div id="sidebar">

            <div class="inner">

                <!-- Search Box -->
                <section id="search" class="alt">
                    <form method="get" action="#">
                        <input type="text" name="search" id="search" placeholder="Search..." />
                    </form>
                </section>
                <!-- Menu -->
                <nav id="menu">
                    <ul>
                        <li style="color:white">Navigation</li><li></li>
                        <li>@Html.ActionLink("Home", "AdminHome", "HusnaAdmin")</li>

                        @*<li>@Html.ActionLink("Schedule", "IndexSchedule", "Schedule")</li>*@

                        <li>
                            <span class="opener">Management</span>
                            <ul>
                                <li>@Html.ActionLink("Staff Management", "IndexStaffAdmin", "HusnaAdmin")</li>
                                <li>@Html.ActionLink("Student Management", "IndexStudentAdmin", "HusnaAdmin")</li>
                            </ul>
                        </li>
                        <li>
                            <span class="opener">Report</span>
                            <ul>
                                <li>@Html.ActionLink("Student Report", "IndexStudent", "HusnaAdmin")</li>
                                <li>@Html.ActionLink("DASS Report", "DASSresult", "HusnaAdmin")</li>
                            </ul>
                        </li>
                        <li>@Html.ActionLink("LOGOUT", "index", "HaoKeatHome")</li>
                    </ul>
                </nav>

                <!-- Start Footer  -->
                <footer id="footer">
                    <p class="copyright">
                        Admin Module
                        <br>Husna Hashiki</a>
                    </p>
                </footer>

            </div>
        </div>

    </div>
    <!-- End Footer  -->
    <!-- End copyright  -->
    <!-- ALL JS FILES -->
    <script src="~/Scripts/js/jquery.min.js"></script>
    <script src="~/Scripts/js/bootstrap.bundle.min.js"></script>

    <script src="~/Scripts/js/browser.min.js"></script>
    <script src="~/Scripts/js/breakpoints.min.js"></script>
    <script src="~/Scripts/js/transition.js"></script>
    <script src="~/Scripts/js/owl-carousel.js"></script>
    <script src="~/Scripts/js/custom.js"></script>
</body>
</html>
