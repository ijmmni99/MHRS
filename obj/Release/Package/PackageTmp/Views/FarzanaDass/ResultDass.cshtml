@model List<MHRS.Models.DassForm>
@{
    ViewBag.Title = "ResultDass";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Your Dass Result</h2>

<table class="table">
    <tr>
        <th>
            Date
        </th>
        <th>
            Result Stress
        </th>
        <th>
            Result Anxiety
        </th>
        <th>
            Result Depression
        </th>
        <th>
            Condition
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Result_Stress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Result_Anxiety)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Result_Depression)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Condition)
            </td>
        </tr>
    }

</table>